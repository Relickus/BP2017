<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root fx:id="resultItem" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
     <children>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <children>
                  <VBox maxWidth="185.0" minWidth="185.0" prefWidth="185.0" spacing="20.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Label text="Info:">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <font>
                              <Font name="System Italic" size="12.0" />
                           </font>
                        </Label>
                        <Separator prefWidth="100.0" />
                        <HBox>
                           <children>
                              <VBox>
                                 <children>
                                    <Label text="Solver:" />
                                    <Label fx:id="solverName" text="..." wrapText="true">
                                       <font>
                                          <Font name="Arial Bold" size="15.0" />
                                       </font>
                                 <VBox.margin>
                                    <Insets left="25.0" top="10.0" />
                                 </VBox.margin>
                                    </Label>
                                    <Label text="Parameters:">
                                 <VBox.margin>
                                    <Insets top="20.0" />
                                 </VBox.margin></Label>
                                    <Label fx:id="solverParam" text="..." wrapText="true">
                                       <font>
                                          <Font name="Arial" size="15.0" />
                                       </font>
                                 <VBox.margin>
                                    <Insets left="25.0" top="10.0" />
                                 </VBox.margin>
                                    </Label>
                                    <Label text="Accuracy:">
                                 <VBox.margin>
                                    <Insets top="20.0" />
                                 </VBox.margin></Label>
                                    <Label fx:id="solverAcc" text="...">
                                       <font>
                                          <Font name="Arial" size="15.0" />
                                       </font>
                                 <VBox.margin>
                                    <Insets left="25.0" top="10.0" />
                                 </VBox.margin>
                                    </Label>
                                 </children>
                              </VBox>
                              <VBox layoutX="10.0" layoutY="10.0" spacing="70.0">
                                 <padding>
                                    <Insets left="10.0" />
                                 </padding>
                              </VBox>
                           </children>
                        </HBox>
                     </children>
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding>
                  </VBox>
                  <HBox HBox.hgrow="ALWAYS" />
            <HBox fx:id="captchaContainer" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" />
               </children>
            </HBox>
            <Separator prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin></Separator>
         </children>
</fx:root>
